{
  "type": "page",
  "title": "报警日志",
  "body": [
    {
      "type": "crud",
      "draggable": true,
      "api": {
        "method": "get",
        "url": "/api/WCSMS/logDeviceAlarm/getList",
        "requestAdaptor": "",
        "adaptor": "return {\r\n  \"status\": 0,\r\n  \"msg\": \"\",\r\n  \"data\": {\r\n    \"items\": response.data.data.items,\r\n    \"total\": response.data.data.totalCount\r\n  }\r\n}",
        "messages": {},
        "replaceData": false,
        "data": {
          "&": "$$",
          "skipCount": "${(page - 1) * perPage}",
          "maxResultCount": "${perPage}",
          "partNo": "${partNo}",
          "partLocNo": "${partLocNo}",
          "devNo": "${devNo}",
          "alarmCode": "${alarmCode}",
          "alarmDesc": "${alarmDesc}"
        }
      },
      "columns": [
        {
          "name": "devName",
          "label": "设备号",
          "searchable": {
            "type": "input-text",
            "name": "devName",
            "label": "设备号"
          },
          "type": "text"
        },
        {
          "name": "partName",
          "label": "部件名称",
          "searchable": {
            "type": "input-text",
            "name": "partNo",
            "label": "部件号"
          },
          "type": "text"
        },
        {
          "name": "partLocNo",
          "label": "站台号",
          "type": "text"
        },
        {
          "name": "alarmCode",
          "label": "报警代码",
          "searchable": {
            "type": "input-text",
            "name": "alarmCode",
            "label": "报警代码"
          },
          "type": "text"
        },
        {
          "name": "alarmDesc",
          "label": "报警信息",
          "searchable": {
            "type": "input-text",
            "name": "alarmDesc",
            "label": "报警信息"
          },
          "type": "text"
        },
        {
          "name": "handleFlagDesc",
          "label": "处理标志",
          "searchable": {
            "type": "select",
            "options": [
              {
                "label": "已处理",
                "value": 1
              },
              {
                "label": "未处理",
                "value": 0
              }
            ],
            "name": "handleFlagDesc",
            "label": "处理标志"
          },
          "type": "text"
        },
        {
          "name": "beginTime",
          "label": "开始时间",
          "type": "text",
          "searchable": {
            "type": "input-datetime",
            "name": "beginTimeStart",
            "label": "开始创建时间",
            "format": "YYYY-MM-DD HH:mm:ss"
          }
        },
        {
          "name": "beginTimeEnd",
          "label": "结束创建时间",
          "type": "text",
          "toggled": false,
          "searchable": {
            "type": "input-datetime",
            "name": "beginTimeEnd",
            "label": "结束创建时间",
            "format": "YYYY-MM-DD HH:mm:ss"
          }
        },
        {
          "name": "endTime",
          "label": "结束时间",
          "type": "text"
        }
      ],
      "bulkActions": [],
      "features": ["filter", "update", "create", "bulkDelete"],
      "filterColumnCount": 3,
      "autoGenerateFilter": {
        "columnsNum": 4,
        "showBtnToolbar": true
      },
      "footable": {
        "expand": "first",
        "accordion": true
      },
      "perPageAvailable": [10],
      "messages": {
        "fetchSuccess": "查询成功",
        "fetchFailed": ""
      },
      "hidden": false,
      "headerToolbar": [
        {
          "type": "bulk-actions"
        }
      ],
      "keepItemSelectionOnPageChange": false,
      "perPage": 10,
      "alwaysShowPagination": true,
      "maxKeepItemSelectionLength": 100,
      "pageField": "page",
      "syncResponse2Query": false
    }
  ],
  "regions": ["body", "header"],
  "asideResizor": false,
  "pullRefresh": {
    "disabled": true
  },
  "aside": []
}
