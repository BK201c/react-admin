      "draggable": true,
      "api": {
        "method": "get",
        "url": "/api/WCSMS/wcsTaskHis/getList",
        "requestAdaptor": "",
        "adaptor": "return {\r\n  \"status\": 0,\r\n  \"msg\": \"\",\r\n  \"data\": {\r\n    \"items\": response.data.data.items,\r\n    \"total\": response.data.data.totalCount\r\n  }\r\n}",
        "messages": {},
        "replaceData": false,
        "data": {
          "&": "$$",
          "skipCount": 0,
          "maxResultCount": 20,
          "taskNo": "${taskNo}",
          "wmsTaskNo": "${wmsTaskNo}",
          "palletBarcode": "${palletBarcode}",
          "taskTypeNo": "${taskTypeNo}",
          "taskStatus": "${taskStatus}",
