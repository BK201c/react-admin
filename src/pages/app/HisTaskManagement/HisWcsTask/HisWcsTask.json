          "slocNo": "${slocNo}",
          "realElocNo": "${realElocNo}",
          "elocNo": "${elocNo}",
          "sbinNo": "${sbinNo}",
          "ebinNo": "${ebinNo}",
          "errCode": "${errCode}",
          "returnFlag": "${returnFlag}",
          "createTimeStart": "${createTimeStart}",
          "createTimeEnd": "${createTimeEnd}"
        }
