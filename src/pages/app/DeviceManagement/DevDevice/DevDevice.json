                      "replaceData": false,
                      "data": {
                        "&": "$$",
                        "id": "${id}",
                        "connectType": "${connectType}",
                        "devName": "${devName}",
                        "devDesc": "${devDesc}",
                        "devNo": "${devNo}",
                        "devTypeNo": "${devTypeNo}",
                        "ipAddress": "${ipAddress}",
                        "ipPort": "${ipPort}",
                        "plcType": "${plcType}",
                        "sdType": "${sdType}",
                        "usedFlag": "${usedFlag}",
                        "wcsNo": "${wcsNo}",
                        "areaNo": "${areaNo}"
                      },
                      "dataType": "json",
                      "headers": {
                        "Accept-Language": "zh-Hans"
                      }
                    },
                    "body": [
                      {
                        "name": "devNo",
                        "label": "设备编码",
                        "type": "input-text",
                        "required": true
                      },
                      {
                        "name": "devName",
                        "label": "设备名称",
                        "type": "input-text",
                        "required": true
                      },
                      {
                        "name": "devDesc",
                        "label": "设备描述",
                        "type": "input-text",
                        "required": true
                      },
                      {
                        "name": "devTypeNo",
