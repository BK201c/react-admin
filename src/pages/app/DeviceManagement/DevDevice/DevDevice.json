{
  "type": "page",
  "title": "设备管理",
  "body": [
    {
      "type": "crud",
      "draggable": true,
      "api": {
        "method": "get",
        "url": "/api/WCSMS/devDevice/getList",
        "requestAdaptor": "",
        "adaptor": "return {\r\n  \"status\": 0,\r\n  \"msg\": \"\",\r\n  \"data\": {\r\n    \"items\": response.data.data.items,\r\n    \"total\": response.data.data.totalCount\r\n  }\r\n}",
        "messages": {},
        "replaceData": false,
        "data": {
          "&": "$$",
          "skipCount": "${(page - 1) * perPage}",
          "maxResultCount": "${perPage}",
          "devNo": "${devNo}",
          "devName": "${devName}",
          "devTypeNo": "${devTypeNo}",
          "wcsNo": "${wcsNo}",
          "usedFlag": "${usedFlag}"
        },
        "dataType": "json",
        "headers": {
          "Accept-Language": "zh-Hans"
        }
      },
      "columns": [
        {
          "name": "id",
          "label": "序号",
          "type": "text"
        },
        {
          "name": "devNo",
          "label": "设备编码",
          "searchable": {
            "type": "input-text",
            "name": "devNo",
            "label": "设备编码"
          },
          "type": "text"
        },
        {
          "name": "devName",
          "label": "设备名称",
          "searchable": {
            "type": "input-text",
            "name": "devName",
            "label": "设备名称"
          },
          "type": "text"
        },
        {
          "name": "devDesc",
          "label": "设备描述",
          "type": "text"
        },
        {
          "name": "devTypeName",
          "label": "设备类型",
          "searchable": {
            "type": "select",
            "source": {
              "method": "get",
              "url": "/api/WCSMS/options/getDevTypeOption",
              "adaptor": "return {\r\n  \"status\": 0,\r\n  \"msg\": \"\",\r\n  \"data\": {\r\n    \"items\": response.data.data.items\r\n  }\r\n}",
              "headers": {
                "Accept-Language": "zh-Hans"
              }
            },
            "name": "devTypeNo",
            "label": "设备类型"
          },
          "type": "text"
        },
        {
          "name": "isConnectDesc",
          "label": "连接标志",
          "type": "text"
        },
        {
          "name": "ipAddress",
          "label": "IP地址",
          "type": "text"
        },
        {
          "name": "ipPort",
          "label": "端口号",
          "type": "text"
        },
        {
          "name": "connectTypeDesc",
          "label": "连接类型",
          "type": "text"
        },
        {
          "name": "plcTypeDesc",
          "label": "PLC类型",
          "type": "text"
        },
        {
          "name": "sdTypeDesc",
          "label": "伸工位类型",
          "type": "text"
        },
        {
          "name": "areaName",
          "label": "区域",
          "type": "text"
        },
        {
          "name": "wcsNo",
          "label": "WCS编号",
          "searchable": {
            "type": "input-text",
            "name": "wcsNo",
            "label": "WCS编号"
          },
          "type": "text"
        },
        {
          "name": "usedFlag",
          "label": "启用标志",
          "type": "mapping",
          "map": {
            "0": "<span class='label label-danger'>禁用</span>",
            "1": "<span class='label label-success'>启用</span>",
            "*": "${usedFlag}"
          },
          "searchable": {
            "type": "select",
            "name": "usedFlag",
            "label": "启用标志",
            "options": [
              {
                "label": "禁用",
                "value": "0"
              },
              {
                "label": "启用",
                "value": "1"
              }
            ]
          }
        },
        {
          "name": "createBy",
          "label": "创建人",
          "type": "text"
        },
        {
          "name": "createTime",
          "label": "创建时间",
          "type": "text"
        },
        {
          "name": "updateBy",
          "label": "更新人",
          "type": "text"
        },
        {
          "name": "updateTime",
          "label": "更新时间",
          "type": "text"
        },
        {
          "type": "operation",
          "label": "操作",
