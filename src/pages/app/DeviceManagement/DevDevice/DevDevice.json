                        "source": {
                          "method": "get",
                          "url": "/api/WCSMS/options/getAreaOption",
                          "adaptor": "return {\r\n  \"status\": 0,\r\n  \"msg\": \"\",\r\n  \"data\": {\r\n    \"items\": response.data.data.items\r\n  }\r\n}",
                          "headers": {
                            "Accept-Language": "zh-Hans"
                          }
                        },
                        "type": "select",
                        "required": true
                      },
                      {
                        "name": "wcsNo",
                        "label": "WCS编号",
                        "type": "input-text",
                        "required": true
                      },
                      {
                        "name": "ipAddress",
                        "label": "IP地址",
                        "type": "input-text",
                        "required": true
                      },
                      {
                        "name": "ipPort",
                        "label": "端口号",
                        "type": "input-text",
                        "required": true
